{
	"name": "honeyos",
	"description": "HoneyOS",
	"version": "0.1.0",
	"main": "public/electron.js",
	"private": true,
	"author": "Big-O Buddy (BOB)",
	"homepage": "./",
	"dependencies": {
		"@electron/remote": "^2.1.2",
		"@mui/material": "^5.15.18",
		"@radix-ui/react-alert-dialog": "^1.0.5",
		"@radix-ui/react-dialog": "^1.0.5",
		"@radix-ui/react-icons": "^1.3.0",
		"@radix-ui/react-label": "^2.0.2",
		"@radix-ui/react-scroll-area": "^1.0.5",
		"@radix-ui/react-select": "^2.0.0",
		"@radix-ui/react-slot": "^1.0.2",
		"@radix-ui/react-tabs": "^1.0.4",
		"@radix-ui/react-toggle": "^1.0.3",
		"@radix-ui/react-toggle-group": "^1.0.4",
		"@radix-ui/react-tooltip": "^1.0.7",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@testing-library/user-event": "^13.5.0",
		"async": "^3.2.5",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.0",
		"cors": "^2.8.5",
		"electron-build": "^0.0.3",
		"electron-updater": "^6.1.8",
		"express": "^4.19.2",
		"express-ws": "^5.0.2",
		"framer-motion": "^12.6.3",
		"fs": "^0.0.1-security",
		"lucide-react": "^0.368.0",
		"node-gyp": "^10.1.0",
		"react": "^18.2.0",
		"react-bootstrap": "^2.10.2",
		"react-dom": "^18.2.0",
		"react-draggable": "^4.4.6",
		"react-router-dom": "^6.22.3",
		"react-scripts": "^5.0.1",
		"react-transition-group": "^4.4.5",
		"sass": "^1.77.2",
		"tailwind-merge": "^2.2.2",
		"tailwindcss-animate": "^1.0.7",
		"web-vitals": "^2.1.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"rebuild-app": "electron-builder install-app-deps",
		"electron": "wait-on tcp:3000 && electron .",
		"start-electron-app": "concurrently -k \"cross-env BROWSER=none npm run start\" \"cross-env NODE_ENV=development  npm run electron\"",
		"package-windows": "rimraf build && rimraf dist && npm run build && electron-builder -w -c.extraMetadata.main=build/electron.js",
		"package-mac": "rimraf build && rimraf dist && npm run build && electron-builder build  -m -c.extraMetadata.main=build/electron.js ",
		"package-linux": "rimraf build && rimraf dist && npm run build && electron-builder -l -c.extraMetadata.main=build/electron.js"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/plugin-proposal-private-property-in-object": "^7.21.11",
		"concurrently": "^8.1.0",
		"cross-env": "^7.0.3",
		"electron": "^35.1.5",
		"electron-builder": "^24.13.3",
		"tailwindcss": "^3.4.3",
		"wait-on": "^7.0.1"
	},
	"optionalDependencies": {
		"dmg-license": "^1.0.11"
	},
	"build": {
		"appId": "honeyos.app",
		"mac": {
			"extendInfo": {
				"NSCameraUseContinuityCameraDeviceType": true
			}
		},
		"icon": "public/logo.icns",
		"productName": "HoneyOS",
		"files": [
			"build/**/*",
			"node_modules/**/*",
			"public/**/*",
			"src/**/*",
			"!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
			"!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
			"!**/node_modules/.bin",
			"!**/node_modules/**/*.d.ts",
			"!**/node_modules/**/*.js.map",
			"!**/.git",
			"!**/.DS_Store"
		],
		"directories": {
			"buildResources": "public"
		}
	}
}
